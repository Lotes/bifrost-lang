node Fibonacci(
  in n of Integer,
  out result of Integer
) { 
  match this.n with {
    case 0 {
      link 0 to this.result;
    }
    case 1 {
      link 1 to this.result;
    }
    case x? {
      instance minusOne of Fibonacci;
      instance minusTwo of Fibonacci;
      link x - 1 to minusOne.n;
      link x - 2 to minusTwo.n;
      link minusOne.result + minusTwo.result to this.result;
    }
  }
}